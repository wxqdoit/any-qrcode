{
  "manifest_version": 3,
  "name": "Any QRCode",
  "version": "1.0.0",
  "description": "将选中的文字生成二维码",
  "permissions": ["contextMenus", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Any QRCode"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
